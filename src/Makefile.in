#
#	Makefile for n8
#

PROG = n8
SED = sed

PREFIX=@prefix@

OPTS = -DSYSCONFDIR=\"@sysconfdir@/n8\" -DPROG=${PROG}
mkinstalldirs = /bin/sh $(top_srcdir)/mkinstalldirs

CC = @CC@
DEFS = @DEFS@
LIBS = -lele @LIBS@
# CFLAGS= @CFLAGS@

prefix = @prefix@
top_srcdir = @top_srcdir@
INSTALL = @INSTALL@
INSTALL_DATA = @INSTALL_DATA@
#INSTALL_PROGRAM = @INSTALL_PROGRAM@ -s
INSTALL_PROGRAM = @INSTALL_PROGRAM@
bindir = $(prefix)/@bindir@


SRCS = block.c crt.c cursor.c disp.c file.c filer.c \
	input.c iskanji.c keyf.c line.c \
	lineedit.c list.c menu.c \
	search.c setopt.c keys.c sh_shells.c \
	$(PROG).c
OBJS = $(SRCS:.c=.o)

all: $(PROG)

$(PROG): $(OBJS) ../lib/libele.a
	$(CC) -L../lib -o $@ $(OBJS) $(LIBS)

.c.o:
	@echo $(CC) $(CFLAGS) -c $<
	@$(CC) $(CFLAGS) $(OPTS) -c $<

clean:
	rm -f $(OBJS) $(PROG)

distclean:
	rm -f $(OBJS) $(PROG) \
	      Makefile config.h

install: $(PROG)
	@$(mkinstalldirs) $(bindir)
	$(INSTALL_PROGRAM) $(PROG) $(bindir)/$(PROG)

